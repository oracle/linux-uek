config RDS
	tristate "Reliable Datagram Sockets (RDS) (EXPERIMENTAL)"
	depends on INET && INFINIBAND && INFINIBAND_IPOIB && INFINIBAND_ADDR_TRANS
	---help---
	  RDS provides reliable, sequenced delivery of datagrams
	  over Infiniband.

config RDS_DEBUG
        bool "Debugging messages"
	depends on RDS
        default n
